<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="news.model.News">
	
	<insert id="insertNews">
		
		insert into alarm values(alarm_news.NEXTVAL, #{fk_recipientno}, #{url}, #{url2}, #{alarm_content}, #{alarm_type}, #{view_status} , sysdate)
		
	</insert>                                                                                   
	
</mapper>